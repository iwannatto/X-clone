{
    "name": "X-clone",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "dev",
    "workspaceFolder": "/workspace",
    "postCreateCommand": "corepack enable && npm install -g aws-cdk",
    "customizations": {
      "vscode": {
        "extensions": [
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode",
          "amazonwebservices.aws-toolkit-vscode"
        ]
      }
    }
  }